{
    "name": "yepwoo/laragine",
    "description": "Laravel Engine: Develop Faster And Cleaner!",
    "keywords": [
        "yepwoo",
        "laragine",
        "laravel",
        "module",
        "modules",
        "fast",
        "faster",
        "clean code"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/yepwoo/laragine/issues",
        "source": "https://github.com/yepwoo/laragine"
    },
    "authors": [
      {
          "name": "Mahmood Ahmad",
          "email": "mahmood073@gmail.com"
      },
      {
          "name": "Abdlrahman Saber",
          "email": "abdlrahmansaber652@gmail.com"
      }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "orchestra/testbench": "^6.0",
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Yepwoo\\Laragine\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Yepwoo\\Laragine\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yepwoo\\Laragine\\ModuleServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "test-lv": "vendor/bin/testbench"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
